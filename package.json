{
  "name": "@ctx-core/ctx-core-typescript-tools",
  "version": "7.0.8",
  "description": "Typescript tools for the @ctx-core monorepo",
  "keywords": [
    "ctx-core"
  ],
  "homepage": "https://github.com/ctx-core/ctx-core-typescript-tools#readme",
  "bugs": {
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Takita",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
    "compile": "./node_modules/.bin/tsc -b .",
    "exec": "$@"
  },
  "dependencies": {
    "@ctx-core/array": "workspace:^22.0.9",
    "@ctx-core/object": "workspace:^20.0.8",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@ctx-core/function": "workspace:^19.1.7",
    "esm": "^3.2.25",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public",
    "cache": "~/.npm"
  }
}