{
  "name": "@ctx-core/ctx-core-typescript-tools",
  "version": "7.0.58",
  "description": "Typescript tools for the @ctx-core monorepo",
  "keywords": [
    "ctx-core"
  ],
  "homepage": "https://github.com/ctx-core/ctx-core-typescript-tools#readme",
  "bugs": {
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Takita",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "tsc-config-refactor": "./bin/tsc-config-refactor.mjs"
  },
  "scripts": {
    "build": "npm run copy-mjs && npm run compile",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
    "compile": "./node_modules/.bin/tsc -b .",
    "copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
    "exec": "$@"
  },
  "dependencies": {
    "@ctx-core/array": "workspace:^23.0.41",
    "@ctx-core/object": "workspace:^22.0.8",
    "globby": "^12.0.2"
  },
  "devDependencies": {
    "@ctx-core/function": "workspace:^20.2.20",
    "esm": "^3.2.25",
    "typescript": "next"
  },
  "publishConfig": {
    "access": "public",
    "cache": "~/.npm"
  }
}
