{
  "name": "@ctx-core/ctx-core-typescript-tools",
  "version": "7.0.29",
  "description": "Typescript tools for the @ctx-core monorepo",
  "keywords": [
    "ctx-core"
  ],
  "homepage": "https://github.com/ctx-core/ctx-core-typescript-tools#readme",
  "bugs": {
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctx-core/ctx-core-typescript-tools/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Takita",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "bin": {
    "tsc-config-refactor": "./bin/tsc-config-refactor.mjs"
  },
  "scripts": {
    "build": "npm run clean && npm run copy-mjs && npm run compile",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
    "compile": "./node_modules/.bin/tsc -b .",
    "copy-mjs": "cd dist && echo \"export * from './index.js'\" > index.mjs",
    "exec": "$@"
  },
  "dependencies": {
    "@ctx-core/array": "workspace:^23.0.3",
    "@ctx-core/object": "workspace:^21.0.3",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@ctx-core/function": "workspace:^20.0.3",
    "esm": "^3.2.25",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public",
    "cache": "~/.npm"
  }
}
